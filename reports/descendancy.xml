						<PGVRSetVar name="generation" value="@generation" />
					</PGVRif>
					<PGVRTextBox border="" width="25" height="40">
						<PGVRText style="name"><PGVRvar var="personNumber" />.</PGVRText>
					</PGVRTextBox>

					<PGVRTextBox border="" height="40" newline="1">
