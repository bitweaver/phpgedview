														</PGVRGedcom>
													</PGVRGedcom>
												</PGVRRepeatTag>
											</PGVRif>
											<PGVRGedcom id="DEAT">
												<PGVRText style="label1"> <PGVRvarLetter var="factarray[DEAT]" />: <PGVRGedcomValue tag="DATE" level="2"/> <PGVRGedcomValue tag="PLAC" level="2"/></PGVRText>
											</PGVRGedcom>
										</PGVRTextBox>
									</PGVRGedcom>
									<PGVRSetVar name="familyChildNumber" value="$familyChildNumber+1" />
