									<PGVRGedcomValue tag="SOUR:PAGE"/> <PGVRGedcomValue tag="SOUR:DATA:TEXT"/>
								</PGVRFootnote>
							</PGVRRepeatTag>
						</PGVRif>
						<PGVRSetVar name="printPeriod" value="false" />
